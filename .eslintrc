{
  "extends": "next/core-web-vitals",
  "rules": {
    "linebreak-style": ["warn", "unix"],
    "quotes": ["warn", "single"],
    "semi": ["off", "always"],
    "no-unused-vars": ["warn"],
    // "no-console": "error",
    "no-process-exit": "off",
    "no-underscore-dangle": "off",
    "consistent-return": "off",
    "func-names": "off"
  }
}
