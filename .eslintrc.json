{
  "root": true,
  "extends": ["next/core-web-vitals"],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "rules": {
    "linebreak-style": ["warn", "unix"],
    // "quotes": ["warn", "single"],
    "semi": ["off", "always"],
    "no-unused-vars": ["warn"],
    // "no-console": "error",
    "no-process-exit": "off",
    "no-underscore-dangle": "off",
    "consistent-return": "off",
    "func-names": "off"
  }
}
